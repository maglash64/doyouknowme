@model doyouknowme.Controllers.profileController
@{
     Layout = "~/Views/Shared/_layout.cshtml";
}

<div class="container-fluid" style="background-color: cornflowerblue; display: flex; padding: 15px;">
    <img style="border-radius: 50%; background: white; padding: 5px; height: 128px; width: 128px;" @ViewBag.picture>
    <div style="font-family: Ubuntu; font-weight: 100; font-size: 36px; color:white; padding: 0 0 20px 20px;">
         @ViewBag.name <br>
         <span style="font-size: 24px">@ViewBag.email</span>
    </div>
</div>
<div class="jumbotron jumbotron-fluid text-center">
    <h3>Under Development</h3>
</div>


